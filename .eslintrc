{
    "env": {
        "browser": true,
        "amd": true
    },
    "globals": {
        "document": true,
        "console": true,
        "window": true,
        "navigator": true
    },
    "rules": {
        "brace-style": 1,
        "comma-spacing": 1,
        "consistent-this": 0,
        "curly": 1,
        "eol-last": 0,
        "func-names": 0,
        "func-style": [2, "declaration"],
        "key-spacing": 1,
        "new-cap": 1,
        "new-parens": 2,
        "no-console": 0,
        "no-constant-condition": 1,
        "no-multi-spaces": 0,
        "no-shadow": 1,
        "no-ternary": 0,
        "no-underscore-dangle": 0,
        "no-unused-vars": [1, {"vars": "local", "args": "none"}],
        "no-use-before-define": 0,
        "one-var": 0,
        "quotes": [2, "single"],
        "space-infix-ops": 0,
        "strict": 0,
        "valid-jsdoc": 0
    }
}
